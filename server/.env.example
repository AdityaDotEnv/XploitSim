# Copy to .env and edit for local development
PORT=4000
NODE_ENV=development
SANDBOX=1

# Absolute or relative path to sqlite DB (recommended relative)
DATABASE_FILE=./server/db/xploitsim.sqlite

# JWT secret for development (replace with long random string locally)
JWT_SECRET=dev_jwt_secret_change_this

# Admin password used by seed (optional)
ADMIN_PWD=admin123
